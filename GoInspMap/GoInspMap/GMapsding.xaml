<Window x:Class="GoInspMap.GMapsding"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="GMapsding" Height="700" Width="1000">
    <Grid>
        <Grid.RowDefinitions>
            
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="700"/>
            <ColumnDefinition />
        </Grid.ColumnDefinitions>
        <Frame Content="Frame" Name="Map" Grid.Column="0" HorizontalAlignment="Left" Height="700" VerticalAlignment="Top" Width="700"/>

        <TextBox Name="input" Width="160" Height="50" Grid.Column="1" HorizontalAlignment="Left" Margin="10" VerticalAlignment="Top" />
        <Button Name="submit" Content="Add" Width="100" Height="50" Grid.Column="1" HorizontalAlignment="Right" Margin="10" VerticalAlignment="Top" Click="submit_Click" />
    </Grid>
</Window>
